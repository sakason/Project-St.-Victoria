render_mode blend_mix;

uniform float distortion_scale = 3;
uniform sampler2D tex_frg_17;
uniform float power = 3;
uniform float distortionBend : hint_range(0, 1) = 1;
uniform sampler2D screen_tex_frg_2 : hint_screen_texture;
uniform float intensity = 1;



void fragment() {
// Input:5
	vec2 n_out5p0 = UV;


// FloatParameter:4
	float n_out4p0 = distortion_scale;


// Texture2D:17
	vec4 n_out17p0 = textureLod(tex_frg_17, UV, n_out4p0);


// UVFunc:10
	vec2 n_in10p1 = vec2(1.00000, 1.00000);
	vec2 n_in10p2 = vec2(-0.50000, -0.50000);
	vec2 n_out10p0 = n_in10p2 * n_in10p1 + UV;


// VectorLen:11
	float n_out11p0 = length(n_out10p0);


// FloatParameter:13
	float n_out13p0 = power;


// FloatOp:12
	float n_out12p0 = pow(n_out11p0, n_out13p0);


// FloatOp:15
	float n_out15p0 = n_out17p0.x * n_out12p0;


// FloatFunc:18
	float n_out18p0 = 1.0 - n_out15p0;


// VectorOp:20
	vec2 n_out20p0 = n_out5p0 * vec2(n_out18p0);


// FloatParameter:7
	float n_out7p0 = distortionBend;


// Mix:6
	vec2 n_out6p0 = mix(n_out5p0, n_out20p0, vec2(n_out7p0));


	vec4 n_out2p0;
// Texture2D:2
