// FloatParameter:21
	float n_out21p0 = intensity;


// FloatOp:22
	float n_out22p0 = n_out18p0 * n_out21p0;


// VectorOp:23
	vec3 n_out23p0 = vec3(n_out2p0.xyz) + vec3(n_out22p0);


// Output:0
	COLOR.rgb = n_out23p0;


}
